	<modelVersion>4.0.0</modelVersion>
	<parent>
      <groupId>com.meizu</groupId>
      <artifactId>simplify-framework</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </parent>
    <artifactId>stress-tester</artifactId>
    <packaging>jar</packaging>
	<name>方法级别压力测试组件</name>
    <description>用于开发阶段的压力测试</description>
	<properties>
  	  <templatePath>${project.parent.basedir}</templatePath>
  	  <parentsite></parentsite>
  	</properties>
	<!-- 缺少用户名和密码的设置，暂时不可用 -->
